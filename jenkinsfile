pipeline {
agent any
    stages {
        stage('Pull') {
            steps {
                script {
                    git branch: 'main', url: 'https://github.com/bendohou/imageDocker.git'
                }
            }
        }

        stage('remove container') {
                    steps {
                        script {
                            sh 'docker kill appli3'
                            sh 'docker rm appli3'
                        }
                    }
           }

        stage('run service') {
            steps {
                script {
                    sh 'docker compose up -d '
                }
            }
        }
        
       
        
        
        
        
        
       
    }
}

